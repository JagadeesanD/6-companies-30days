from itertools import combinations
class Solution(object):
    def combinationSum3(self, k, n):
        """
        :type k: int
        :type n: int
        :rtype: List[List[int]]
        """
        lst=[1,2,3,4,5,6,7,8,9]
        return [pair for pair in combinations(lst, k) if sum(pair) == n]
       
       
       //evaluated the combinations sumIII in python using the combination function
